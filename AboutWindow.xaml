<Window x:Class="RayBin.FishImageLabel.AboutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:RayBin.FishImageLabel"
        mc:Ignorable="d"
        WindowStyle="None"
        BorderThickness="0"
        AllowsTransparency="True"
        WindowStartupLocation="CenterScreen"
        Topmost="True"
        ShowInTaskbar="False"
        Title="About" Width="680" Height="420"
        MouseUp="CloseAboutWindow">
    <Grid>
        <Label Name="labAuthorize" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" RenderTransformOrigin="18.5,9.4" Margin="55,230,0,0"/>
        <Label Name="labVersion" Content="" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="White" RenderTransformOrigin="18.5,9.4" Margin="55,260,0,0"/>        
    </Grid>
    <Window.Background>
        <ImageBrush ImageSource="/Resources/SplashScreen.jpg"/>
    </Window.Background>
</Window>
